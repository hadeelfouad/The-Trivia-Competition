{
  "name": "riseup_competition",
  "main": "express.js",
  "scripts": {
    "start": "gulp serve"
  },
  "dependencies": {
    "angular2-jwt": "^0.2.3",
    "babel-core": "^6.26.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "~1.18.3",
    "cors": "^2.8.4",
    "del": "^3.0.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-jwt": "5.1.0",
    "express-session": "^1.15.6",
    "express-validation": "^1.0.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-newer": "^1.4.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-sourcemaps": "^2.6.4",
    "joi": "^13.4.0",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "mongodb": "^3.1.1",
    "mongoose": "^5.1.4",
    "nodemailer": "^4.6.7",
    "os": "^0.1.1",
    "postman": "^0.2.0",
    "run-sequence": "^2.2.1"
  },
  "devDependencies": {
    "nodemon": "^1.12.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "babel-polyfill"
    ],
    "plugins": [
      "add-module-exports",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ],
      [
        "transform-regenerator",
        {
          "asyncGenerators": true,
          "generators": true,
          "async": true
        }
      ],
      "babel-polyfill",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  }
}